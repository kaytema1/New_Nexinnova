                            <li id="text-6" class="widget widget_text"><h3>Contact Form</h3>			<div class="textwidget"><p>Support questions will ONLY be answered via ThemeForest</p>

<script type='text/javascript'>
(function ($) {
$(document).ready(function(){
 $('#footercontactcontactform input').focus(function() {
	$(this).removeClass('fielderror');
 });
 $('#footercontactcontactform textarea').focus(function() {
	$(this).removeClass('fielderror');
 });
	
 $("#footercontactcontactform #footercontactsubmit").click(function(){

  $("#footercontactcontactform_wrap .error").slideUp();
  var hasError = false;
  var email_regex = /^([a-z0-9_\.-]+)@([\da-z\.-]+)\.([a-z\.]{2,6})$/;

  var emailToVal = $("#footercontactemailTo").val();
  emailToVal=emailToVal.replace('#','@');
  if(emailToVal == '') {
   $("#footercontactemailTo").addClass('fielderror');
   hasError = true;
  } else if(!email_regex.test(emailToVal)) {
   $("#footercontactemailTo").addClass('fielderror');
   hasError = true;
  }

  var emailFromVal = $("#footercontactemailFrom").val();
  
  if(emailFromVal == '') {
   $("#footercontactemailFrom").addClass('fielderror');
   hasError = true;
  } else if(!email_regex.test(emailFromVal)) {
   $("#footercontactemailFrom").addClass('fielderror');
   hasError = true;
  }

  var subjectVal = $("#footercontactsubject").val();

  var messageVal = $("#footercontactmessage").val();

  if(messageVal == '') {
   $("#footercontactmessage").addClass('fielderror');
   hasError = true;
  }

  var nameVal = $("#footercontactname").val();
  if(nameVal == '') {
   $("#footercontactname").addClass('fielderror');
   hasError = true;
  }
  

  if(hasError == false) {
			
   $.post('http://dynamix.creativeworkz.co.uk/wp-content/themes/DynamiX/lib/inc/contact-send.php',$("#footercontactcontactform").serialize(),

     function(data){
		$("#footercontactcontactform_wrap .error").slideUp();		
		$('#footercontactcontactform .field').val('');
		$("#footercontactcontactform_wrap .success").slideDown(function() {
			$(this).delay(5000).slideUp('slow');	
		});
    }
   );
  } else if(hasError == true) {
	  $("#footercontactcontactform_wrap .error").slideDown();
  }

  return false;
 });
}); 

})(jQuery);
</script>

